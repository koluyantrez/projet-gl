{"ast":null,"code":"import LogoHome from './LogoHome.vue';\nimport NotifIcon from './NotifIcon.vue';\nimport { useStore } from 'vuex';\nimport { computed, ref } from 'vue';\nimport fr from '../views/fr.js';\nimport en from '../views/en.js';\nexport default {\n  components: {\n    LogoHome: LogoHome,\n    NotifIcon: NotifIcon\n  },\n  setup: function setup() {\n    var store = useStore();\n    var idLa = computed(function () {\n      return store.state.lang.curLang;\n    });\n    var cLang = ref(idLa.value === 'fr' ? fr : en);\n    var switchLang = function switchLang() {\n      if (idLa.value === 'fr') {\n        store.commit('setLang', 'en');\n      } else if (idLa.value === 'en') {\n        store.commit('setLang', 'fr');\n      }\n    };\n    return {\n      switchLang: switchLang,\n      cLang: cLang,\n      src1: require('../assets/profil.png'),\n      src2: require('../assets/illumis.png')\n    };\n  }\n};","map":{"version":3,"names":["LogoHome","NotifIcon","useStore","computed","ref","fr","en","components","setup","store","idLa","state","lang","curLang","cLang","value","switchLang","commit","src1","require","src2"],"sources":["/home/koluyantrez/Documents/bac3/gl/Projet-developpement-logiciel-2024-main/src/main/java/com/genieLogiciel/Umons/frontend/src/elements/MoodleTop.vue"],"sourcesContent":["<template>\n  <div id=\"rectangle\" class=\"redtop\">\n    <NotifIcon/>\n    <LogoHome :src=\"src2\"/>\n    <img class=\"la\" alt=\"Change the language\" src=\"../assets/lang.png\" @click=\"switchLang\"/>\n  </div>\n</template>\n\n\n<script>\nimport LogoHome from './LogoHome.vue';\nimport NotifIcon from './NotifIcon.vue';\nimport { useStore } from 'vuex';\nimport { computed, ref } from 'vue';\nimport fr from '../views/fr.js';\nimport en from '../views/en.js';\n\n\n\nexport default {\n  components: {LogoHome, NotifIcon},\n  setup() {\n\n    const store = useStore();\n    const idLa = computed(() => store.state.lang.curLang);\n    const cLang = ref(idLa.value === 'fr' ? fr : en);\n\n    const switchLang = () => {\n        if (idLa.value === 'fr') {\n            store.commit('setLang', 'en');\n        }\n        else if (idLa.value === 'en') {\n            store.commit('setLang', 'fr');\n        }\n    };\n\n\n    return {\n      switchLang,\n      cLang,\n      src1: require('../assets/profil.png'),\n      src2: require('../assets/illumis.png'),\n    }\n  },\n  }\n</script>\n\n<style scoped>\n.redtop {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100px;\n  background-color: rgb(158, 11, 23);\n  z-index: 90;\n}\n\n.la{\n  position: fixed;\n  top: 1rem;\n  left: 13rem;\n  width: 4rem; \n  height: auto;\n  z-index: 91;\n}\n</style>\n\n<style>\nbody{\n  background-image: url('../assets/bgHome.png');\n}\n</style>\n\n"],"mappings":"AAUA,OAAOA,QAAO,MAAO,gBAAgB;AACrC,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,SAASC,QAAO,QAAS,MAAM;AAC/B,SAASC,QAAQ,EAAEC,GAAE,QAAS,KAAK;AACnC,OAAOC,EAAC,MAAO,gBAAgB;AAC/B,OAAOC,EAAC,MAAO,gBAAgB;AAI/B,eAAe;EACbC,UAAU,EAAE;IAACP,QAAQ,EAARA,QAAQ;IAAEC,SAAS,EAATA;EAAS,CAAC;EACjCO,KAAK,WAAAA,MAAA,EAAG;IAEN,IAAMC,KAAI,GAAIP,QAAQ,CAAC,CAAC;IACxB,IAAMQ,IAAG,GAAIP,QAAQ,CAAC;MAAA,OAAMM,KAAK,CAACE,KAAK,CAACC,IAAI,CAACC,OAAO;IAAA,EAAC;IACrD,IAAMC,KAAI,GAAIV,GAAG,CAACM,IAAI,CAACK,KAAI,KAAM,IAAG,GAAIV,EAAC,GAAIC,EAAE,CAAC;IAEhD,IAAMU,UAAS,GAAI,SAAbA,UAASA,CAAA,EAAU;MACrB,IAAIN,IAAI,CAACK,KAAI,KAAM,IAAI,EAAE;QACrBN,KAAK,CAACQ,MAAM,CAAC,SAAS,EAAE,IAAI,CAAC;MACjC,OACK,IAAIP,IAAI,CAACK,KAAI,KAAM,IAAI,EAAE;QAC1BN,KAAK,CAACQ,MAAM,CAAC,SAAS,EAAE,IAAI,CAAC;MACjC;IACJ,CAAC;IAGD,OAAO;MACLD,UAAU,EAAVA,UAAU;MACVF,KAAK,EAALA,KAAK;MACLI,IAAI,EAAEC,OAAO,CAAC,sBAAsB,CAAC;MACrCC,IAAI,EAAED,OAAO,CAAC,uBAAuB;IACvC;EACF;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}