{"ast":null,"code":"import TopSec from '../../elements/TopSec.vue';\nimport ItemAdd from '../../elements/ItemAdd.vue';\nimport YesOrNo from '../../popup/YesOrNo.vue';\nimport RenameCo from '../../popup/RenameCo.vue';\nimport { useStore } from 'vuex';\nimport { computed, watch, ref } from 'vue';\nimport fr from '../../views/fr.js';\nimport en from '../../views/en.js';\n//import Cours from '../../model/Cours.java';\n\nexport default {\n  components: {\n    TopSec: TopSec,\n    ItemAdd: ItemAdd,\n    YesOrNo: YesOrNo,\n    RenameCo: RenameCo\n  },\n  setup: function setup() {\n    var store = useStore();\n    var idLa = computed(function () {\n      return store.state.lang.curLang;\n    });\n    var cLang = ref(idLa.value === 'fr' ? fr : en);\n    watch(idLa, function (newLang) {\n      cLang.value = newLang === 'fr' ? fr : en;\n    });\n    var popupUnsub = ref({\n      buTriUnsub: false\n    });\n    var ToUnsubPopup = function ToUnsubPopup(tri1) {\n      popupUnsub.value[tri1] = !popupUnsub.value[tri1];\n    };\n    var NameCours = ref({\n      buRename: false\n    });\n    var ToRenamePopup = function ToRenamePopup(tri) {\n      NameCours.value[tri] = !NameCours.value[tri];\n    };\n    return {\n      cLang: cLang,\n      popupUnsub: popupUnsub,\n      ToUnsubPopup: ToUnsubPopup,\n      NameCours: NameCours,\n      ToRenamePopup: ToRenamePopup,\n      name: 'S-INFO-810 Quantum Computing',\n      prof: 'Seweryn Dynerowicz',\n      assi: [{\n        a: 'Julien Ladeuze'\n      }, {\n        a: 'Maxime Bazin'\n      }, {\n        a: 'Kacem Barkani'\n      }]\n    };\n  }\n  /* methods:{\n      getAllData(){\n          Cours.getAllData().then((response)=>{this.cours=response.data})\n      }\n      },\n      created(){\n          this.getAllData()\n      } */\n};","map":{"version":3,"names":["TopSec","ItemAdd","YesOrNo","RenameCo","useStore","computed","watch","ref","fr","en","components","setup","store","idLa","state","lang","curLang","cLang","value","newLang","popupUnsub","buTriUnsub","ToUnsubPopup","tri1","NameCours","buRename","ToRenamePopup","tri","name","prof","assi","a"],"sources":["/home/koluyantrez/Documents/bac3/gl/Projet-developpement-logiciel-2024-main/src/main/java/com/genieLogiciel/Umons/frontend/src/components/everyone/CoursSec.vue"],"sourcesContent":["<template>\n  <TopSec/>\n  <div class=\"add\"><ItemAdd :word=\"cLang.Courses.rm\" @click=\"() => ToUnsubPopup('buTriUnsub')\"/></div>\n  <div class=\"tit\"><ItemAdd :word=\"cLang.Courses.prof\" @click=\"() => ToRenamePopup('buRename')\"/></div>\n  <div class=\"w\">\n    <h1>{{name}}</h1> <!--co.code+co.name-->\n  </div>\n  <div class=\"info\">\n    <h2>{{ cLang.Courses.prof }} : {{ prof }}</h2> <!--cours.prof-->\n    <h3>{{ cLang.Courses.assi }}<li v-for=\"(item, index) in assi\" :key=\"index\">{{ item.a }}</li></h3>\n    <!--<li v-for=\"co in cours\" v-bind:key=\"co.assistant\">{{ co.assistant }}</li>-->\n  </div>\n  <RenameCo v-if = \"NameCours.buRename\" :ToRenamePopup=\"() => ToRenamePopup('buRename')\" />\n  <YesOrNo v-if = \"popupUnsub.buTriUnsub\" :ToUnsubPopup=\"() => ToUnsubPopup('buTriUnsub')\" />\n</template>\n<script>\nimport TopSec from '../../elements/TopSec.vue';\nimport ItemAdd from '../../elements/ItemAdd.vue';\nimport YesOrNo from '../../popup/YesOrNo.vue';\nimport RenameCo from '../../popup/RenameCo.vue';\nimport { useStore } from 'vuex';\nimport { computed, watch, ref } from 'vue';\nimport fr from '../../views/fr.js';\nimport en from '../../views/en.js';\n//import Cours from '../../model/Cours.java';\n\nexport default{\n  components: {TopSec,ItemAdd,YesOrNo,RenameCo},\n  setup() {\n\n    const store = useStore();\n    const idLa = computed(() => store.state.lang.curLang);\n    const cLang = ref(idLa.value === 'fr' ? fr : en);\n\n    watch(idLa, (newLang) => {\n      cLang.value = newLang === 'fr' ? fr : en;\n    });\n\n    const popupUnsub = ref({\n      buTriUnsub: false\n    });\n    const ToUnsubPopup = (tri1)=>{\n      popupUnsub.value[tri1]=!popupUnsub.value[tri1]\n    }\n\n    const NameCours = ref({\n      buRename: false\n    });\n    const ToRenamePopup = (tri)=>{\n      NameCours.value[tri]=!NameCours.value[tri]\n    }\n\n\n\n    return{\n      cLang,\n      popupUnsub,\n      ToUnsubPopup,\n      NameCours,\n      ToRenamePopup,\n      name: 'S-INFO-810 Quantum Computing',\n      prof: 'Seweryn Dynerowicz',\n      assi: [\n        {a: 'Julien Ladeuze'},\n        {a: 'Maxime Bazin'},\n        {a: 'Kacem Barkani'}\n      ],\n\n    }\n  }\n  /* methods:{\n      getAllData(){\n          Cours.getAllData().then((response)=>{this.cours=response.data})\n      }\n      },\n      created(){\n          this.getAllData()\n      } */\n}\n</script>\n<style scoped>\n.w{\n  position: absolute;\n  left: 110px;\n  top: 70px;\n}\n\n.info{\n  position: absolute;\n  left: 120px;\n  top: 225px;\n  color: rgb(158, 11, 23);\n  font-family: Roboto,sans-serif;\n  font-size: 43px;\n}\n.add{\n  position: absolute;\n  top: 150px;\n  right: 50px;\n}\n.tit{\n  position: absolute;\n  top: 150px;\n  right: 250px;\n}\n\n</style>\n<style>\nh1{\n  color: rgb(158, 11, 23);\n  font-family: Roboto,sans-serif;\n  font-weight: 450;\n  font-size: 90px;\n}\nh2{\n  color: rgb(158, 11, 23);\n  font-family: Roboto,sans-serif;\n  font-weight: 450;\n  font-size: 30px;\n}\nh3{\n  color: rgb(158, 11, 23);\n  font-family: Roboto,sans-serif;\n  font-weight: 300;\n  font-size: 30px;\n}\n</style>"],"mappings":"AAgBA,OAAOA,MAAK,MAAO,2BAA2B;AAC9C,OAAOC,OAAM,MAAO,4BAA4B;AAChD,OAAOC,OAAM,MAAO,yBAAyB;AAC7C,OAAOC,QAAO,MAAO,0BAA0B;AAC/C,SAASC,QAAO,QAAS,MAAM;AAC/B,SAASC,QAAQ,EAAEC,KAAK,EAAEC,GAAE,QAAS,KAAK;AAC1C,OAAOC,EAAC,MAAO,mBAAmB;AAClC,OAAOC,EAAC,MAAO,mBAAmB;AAClC;;AAEA,eAAc;EACZC,UAAU,EAAE;IAACV,MAAM,EAANA,MAAM;IAACC,OAAO,EAAPA,OAAO;IAACC,OAAO,EAAPA,OAAO;IAACC,QAAQ,EAARA;EAAQ,CAAC;EAC7CQ,KAAK,WAAAA,MAAA,EAAG;IAEN,IAAMC,KAAI,GAAIR,QAAQ,CAAC,CAAC;IACxB,IAAMS,IAAG,GAAIR,QAAQ,CAAC;MAAA,OAAMO,KAAK,CAACE,KAAK,CAACC,IAAI,CAACC,OAAO;IAAA,EAAC;IACrD,IAAMC,KAAI,