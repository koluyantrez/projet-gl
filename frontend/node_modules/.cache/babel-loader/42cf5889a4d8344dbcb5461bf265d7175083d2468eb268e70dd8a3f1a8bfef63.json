{"ast":null,"code":"import TopSec from '../../elements/TopSec.vue';\nimport ItemCours from '../../elements/ItemStudent.vue';\nimport ItemSearch from '../../elements/ItemSearch.vue';\nimport ItemAdd from '../../elements/ItemAdd.vue';\nimport AddCours from '../../popup/AddCours.vue';\nimport { useStore } from 'vuex';\nimport { computed, watch, ref, onMounted } from 'vue';\nimport fr from '../../views/fr.js';\nimport en from '../../views/en.js';\nimport axios from \"axios\";\nimport AddStudent from \"@/popup/AddStudent.vue\";\nexport default {\n  components: {\n    AddStudent: AddStudent,\n    ItemCours: ItemCours,\n    TopSec: TopSec,\n    ItemSearch: ItemSearch,\n    ItemAdd: ItemAdd,\n    AddCours: AddCours\n  },\n  setup: function setup() {\n    var students = ref([]);\n    var getStudents = function getStudents() {\n      axios.get('http://localhost:1937/students/getAll').then(function (response) {\n        students.value = response.data;\n      })[\"catch\"](function (error) {\n        console.error(error);\n      });\n    };\n    onMounted(getStudents);\n    var store = useStore();\n    var idLa = computed(function () {\n      return store.state.lang.curLang;\n    });\n    var cLang = ref(idLa.value === 'fr' ? fr : en);\n    watch(idLa, function (newLang) {\n      cLang.value = newLang === 'fr' ? fr : en;\n    });\n    var popupCreate = ref({\n      buCreate: false\n    });\n    var ToCreatePopup = function ToCreatePopup(tri1) {\n      popupCreate.value[tri1] = !popupCreate.value[tri1];\n      if (!popupCreate.value[tri1]) {\n        getStudents();\n      }\n    };\n    return {\n      students: students,\n      popupCreate: popupCreate,\n      ToCreatePopup: ToCreatePopup,\n      cLang: cLang\n    };\n  }\n};","map":{"version":3,"names":["TopSec","ItemCours","ItemSearch","ItemAdd","AddCours","useStore","computed","watch","ref","onMounted","fr","en","axios","AddStudent","components","setup","students","getStudents","get","then","response","value","data","error",