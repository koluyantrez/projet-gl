{"ast":null,"code":"import TopProf from '../../elements/TopProf.vue';\nimport ItemAdd from '../../elements/ItemAdd.vue';\nimport RenameCo from '../../popup/RenameCo.vue';\nimport { useStore } from 'vuex';\nimport { computed, watch, ref } from 'vue';\nimport fr from '../../views/fr.js';\nimport en from '../../views/en.js';\n//import Cours from '../../model/Cours.java';\n\nexport default {\n  components: {\n    TopProf: TopProf,\n    ItemAdd: ItemAdd,\n    RenameCo: RenameCo\n  },\n  setup: function setup() {\n    var store = useStore();\n    var idLa = computed(function () {\n      return store.state.lang.curLang;\n    });\n    var cLang = ref(idLa.value === 'fr' ? fr : en);\n    watch(idLa, function (newLang) {\n      cLang.value = newLang === 'fr' ? fr : en;\n    });\n    var NameCours = ref({\n      buRename: false\n    });\n    var ToRenamePopup = function ToRenamePopup(tri) {\n      NameCours.value[tri] = !NameCours.value[tri];\n    };\n    return {\n      NameCours: NameCours,\n      ToRenamePopup: ToRenamePopup,\n      name: 'S-INFO-810 Quantum Computing',\n      prof: 'Seweryn Dynerowicz',\n      assi: [{\n        a: 'Julien Ladeuze'\n      }, {\n        a: 'Maxime Bazin'\n      }, {\n        a: 'Kacem Barkani'\n      }],\n      cLang: cLang\n    };\n  }\n};","map":{"version":3,"names":["TopProf","ItemAdd","RenameCo","useStore","computed","watch","ref","fr","en","components","setup","store","idLa","state","lang","curLang","cLang","value","newLang","NameCours","buRename","ToRenamePopup","tri","name","prof","assi","a"],"sources":["C:\\Users\\trist\\Desktop\\projet-gl-main\\frontend\\src\\components\\everyone\\CoursProf.vue"],"sourcesContent":["<template>\n  <TopProf/>\n  <div class=\"add\"><ItemAdd :word=\"cLang.Courses.edit\" @click=\"() => ToRenamePopup('buRename')\"/></div>\n  <div class=\"w\">\n    <h1>{{name}}</h1> <!--co.code+co.name-->\n  </div>\n  <div class=\"info\">\n    <h2>{{ cLang.Courses.prof }} : {{ prof }}</h2> <!--cours.prof-->\n    <h3>{{ cLang.Courses.assi }}<li v-for=\"(item, index) in assi\" :key=\"index\">{{ item.a }}</li></h3>\n    <!--<li v-for=\"co in cours\" v-bind:key=\"co.assistant\">{{ co.assistant }}</li>-->\n  </div>\n  <RenameCo v-if = \"NameCours.buRename\" :ToRenamePopup=\"() => ToRenamePopup('buRename')\" />\n</template>\n<script>\nimport TopProf from '../../elements/TopProf.vue';\nimport ItemAdd from '../../elements/ItemAdd.vue';\nimport RenameCo from '../../popup/RenameCo.vue';\nimport { useStore } from 'vuex';\nimport { computed, watch, ref } from 'vue';\nimport fr from '../../views/fr.js';\nimport en from '../../views/en.js';\n//import Cours from '../../model/Cours.java';\n\nexport default{\n  components: {TopProf,ItemAdd,RenameCo},\n  setup() {\n\n    const store = useStore();\n    const idLa = computed(() => store.state.lang.curLang);\n    const cLang = ref(idLa.value === 'fr' ? fr : en);\n\n    watch(idLa, (newLang) => {\n      cLang.value = newLang === 'fr' ? fr : en;\n    });\n\n    const NameCours = ref({\n      buRename: false\n    });\n    const ToRenamePopup = (tri)=>{\n      NameCours.value[tri]=!NameCours.value[tri]\n    }\n\n\n\n    return{\n      NameCours,\n      ToRenamePopup,\n      name: 'S-INFO-810 Quantum Computing',\n      prof: 'Seweryn Dynerowicz',\n      assi: [\n        {a: 'Julien Ladeuze'},\n        {a: 'Maxime Bazin'},\n        {a: 'Kacem Barkani'}\n      ],\n      cLang,\n\n    }\n  }\n}\n</script>\n<style scoped>\n.w{\n  position: absolute;\n  left: 110px;\n  top: 70px;\n}\n\n.info{\n  position: absolute;\n  left: 120px;\n  top: 225px;\n  color: rgb(158, 11, 23);\n  font-family: Roboto,sans-serif;\n  font-size: 43px;\n}\n.add{\n  position: absolute;\n  top: 150px;\n  right: 100px;\n}\n\n</style>\n<style>\nh1{\n  color: rgb(158, 11, 23);\n  font-family: Roboto,sans-serif;\n  font-weight: 450;\n  font-size: 90px;\n}\nh2{\n  color: rgb(158, 11, 23);\n  font-family: Roboto,sans-serif;\n  font-weight: 450;\n  font-size: 30px;\n}\nh3{\n  color: rgb(158, 11, 23);\n  font-family: Roboto,sans-serif;\n  font-weight: 300;\n  font-size: 30px;\n}\n</style>"],"mappings":"AAcA,OAAOA,OAAM,MAAO,4BAA4B;AAChD,OAAOC,OAAM,MAAO,4BAA4B;AAChD,OAAOC,QAAO,MAAO,0BAA0B;AAC/C,SAASC,QAAO,QAAS,MAAM;AAC/B,SAASC,QAAQ,EAAEC,KAAK,EAAEC,GAAE,QAAS,KAAK;AAC1C,OAAOC,EAAC,MAAO,mBAAmB;AAClC,OAAOC,EAAC,MAAO,mBAAmB;AAClC;;AAEA,eAAc;EACZC,UAAU,EAAE;IAACT,OAAO,EAAPA,OAAO;IAACC,OAAO,EAAPA,OAAO;IAACC,QAAQ,EAARA;EAAQ,CAAC;EACtCQ,KAAK,WAAAA,MAAA,EAAG;IAEN,IAAMC,KAAI,GAAIR,QAAQ,CAAC,CAAC;IACxB,IAAMS,IAAG,GAAIR,QAAQ,CAAC;MAAA,OAAMO,KAAK,CAACE,KAAK,CAACC,IAAI,CAACC,OAAO;IAAA,EAAC;IACrD,IAAMC,KAAI,GAAIV,GAAG,CAACM,IAAI,CAACK,KAAI,KAAM,IAAG,GAAIV,EAAC,GAAIC,EAAE,CAAC;IAEhDH,KAAK,CAACO,IAAI,EAAE,UAACM,OAAO,EAAK;MACvBF,KAAK,CAACC,KAAI,GAAIC,OAAM,KAAM,IAAG,GAAIX,EAAC,GAAIC,EAAE;IAC1C,CAAC,CAAC;IAEF,IAAMW,SAAQ,GAAIb,GAAG,CAAC;MACpBc,QAAQ,EAAE;IACZ,CAAC,CAAC;IACF,IAAMC,aAAY,GAAI,SAAhBA,aAAYA,CAAKC,GAAG,EAAG;MAC3BH,SAAS,CAACF,KAAK,CAACK,GAAG,CAAC,GAAC,CAACH,SAAS,CAACF,KAAK,CAACK,GAAG;IAC3C;IAIA,OAAM;MACJH,SAAS,EAATA,SAAS;MACTE,aAAa,EAAbA,aAAa;MACbE,IAAI,EAAE,8BAA8B;MACpCC,IAAI,EAAE,oBAAoB;MAC1BC,IAAI,EAAE,CACJ;QAACC,CAAC,EAAE;MAAgB,CAAC,EACrB;QAACA,CAAC,EAAE;MAAc,CAAC,EACnB;QAACA,CAAC,EAAE;MAAe,EACpB;MACDV,KAAK,EAALA;IAEF;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}