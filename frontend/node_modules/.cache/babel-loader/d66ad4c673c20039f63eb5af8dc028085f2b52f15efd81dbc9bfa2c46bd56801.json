{"ast":null,"code":"import TopGuest from '../../elements/TopGuest.vue';\nimport { useStore } from 'vuex';\nimport { computed, watch, ref } from 'vue';\nimport fr from '../../views/fr.js';\nimport en from '../../views/en.js';\n//import Cours from '../../model/Cours.java';\nexport default {\n  components: {\n    TopGuest: TopGuest\n  },\n  data: function data() {\n    var store = useStore();\n    var idLa = computed(function () {\n      return store.state.lang.curLang;\n    });\n    var cLang = ref(idLa.value === 'fr' ? fr : en);\n    watch(idLa, function (newLang) {\n      cLang.value = newLang === 'fr' ? fr : en;\n    });\n    return {\n      cLang: cLang,\n      name: 'S-INFO-810 Quantum Computing',\n      prof: 'Seweryn Dynerowicz',\n      assi: [{\n        a: 'Julien Ladeuze'\n      }, {\n        a: 'Maxime Bazin'\n      }, {\n        a: 'Kacem Barkani'\n      }]\n    };\n  }\n  /* methods:{\n      getAllData(){\n          Cours.getAllData().then((response)=>{this.cours=response.data})\n      }\n      },\n      created(){\n          this.getAllData()\n      } */\n};","map":{"version":3,"names":["TopGuest","useStore","computed","watch","ref","fr","en","components","data","store","idLa","state","lang","curLang","cLang","value","newLang","name","prof","assi","a"],"sources":["C:\\Users\\trist\\Desktop\\projet-gl-main\\frontend\\src\\components\\everyone\\CoursGuest.vue"],"sourcesContent":["<template>\n  <TopGuest/>\n  <div class=\"w\">\n    <h1>{{name}}</h1> <!--co.code+co.name-->\n  </div>\n  <div class=\"info\">\n    <h2>{{ cLang.Courses.prof }} : {{ prof }}</h2> <!--cours.prof-->\n    <h3>{{ cLang.Courses.assi }}<li v-for=\"(item, index) in assi\" :key=\"index\">{{ item.a }}</li></h3>\n    <!--<li v-for=\"co in cours\" v-bind:key=\"co.assistant\">{{ co.assistant }}</li>-->\n  </div>\n</template>\n<script>\nimport TopGuest from '../../elements/TopGuest.vue';\nimport { useStore } from 'vuex';\nimport { computed, watch, ref } from 'vue';\nimport fr from '../../views/fr.js';\nimport en from '../../views/en.js';\n//import Cours from '../../model/Cours.java';\nexport default {\n  components: {TopGuest},\n  data: () => {\n    const store = useStore();\n    const idLa = computed(() => store.state.lang.curLang);\n    const cLang = ref(idLa.value === 'fr' ? fr : en);\n\n    watch(idLa, (newLang) => {\n      cLang.value = newLang === 'fr' ? fr : en;\n    });\n\n    return {\n      cLang,\n      name: 'S-INFO-810 Quantum Computing',\n      prof: 'Seweryn Dynerowicz',\n      assi: [\n        {a: 'Julien Ladeuze'},\n        {a: 'Maxime Bazin'},\n        {a: 'Kacem Barkani'}\n      ],\n\n    }\n  }\n  /* methods:{\n      getAllData(){\n          Cours.getAllData().then((response)=>{this.cours=response.data})\n      }\n      },\n      created(){\n          this.getAllData()\n      } */\n}\n</script>\n<style scoped>\n.w {\n  position: absolute;\n  left: 110px;\n  top: 70px;\n}\n\n.info {\n  position: absolute;\n  left: 120px;\n  top: 225px;\n  color: rgb(158, 11, 23);\n  font-family: Roboto, sans-serif;\n  font-size: 43px;\n}\n\n</style>\n<style>\nh1 {\n  color: rgb(158, 11, 23);\n  font-family: Roboto, sans-serif;\n  font-weight: 450;\n  font-size: 90px;\n}\n\nh2 {\n  color: rgb(158, 11, 23);\n  font-family: Roboto, sans-serif;\n  font-weight: 450;\n  font-size: 30px;\n}\n\nh3 {\n  color: rgb(158, 11, 23);\n  font-family: Roboto, sans-serif;\n  font-weight: 300;\n  font-size: 30px;\n}\n</style>"],"mappings":"AAYA,OAAOA,QAAO,MAAO,6BAA6B;AAClD,SAASC,QAAO,QAAS,MAAM;AAC/B,SAASC,QAAQ,EAAEC,KAAK,EAAEC,GAAE,QAAS,KAAK;AAC1C,OAAOC,EAAC,MAAO,mBAAmB;AAClC,OAAOC,EAAC,MAAO,mBAAmB;AAClC;AACA,eAAe;EACbC,UAAU,EAAE;IAACP,QAAQ,EAARA;EAAQ,CAAC;EACtBQ,IAAI,EAAE,SAAAA,KAAA,EAAM;IACV,IAAMC,KAAI,GAAIR,QAAQ,CAAC,CAAC;IACxB,IAAMS,IAAG,GAAIR,QAAQ,CAAC;MAAA,OAAMO,KAAK,CAACE,KAAK,CAACC,IAAI,CAACC,OAAO;IAAA,EAAC;IACrD,IAAMC,KAAI,GAAIV,GAAG,CAACM,IAAI,CAACK,KAAI,KAAM,IAAG,GAAIV,EAAC,GAAIC,EAAE,CAAC;IAEhDH,KAAK,CAACO,IAAI,EAAE,UAACM,OAAO,EAAK;MACvBF,KAAK,CAACC,KAAI,GAAIC