{"ast":null,"code":"import MoodleTop from './MoodleTop.vue';\nimport ProfilPhoto from './ProfilPhoto.vue';\nimport TextTravel from './TextTravel.vue';\nimport { useStore } from 'vuex';\nimport { computed, watch, ref } from 'vue';\nimport fr from '../views/fr.js';\nimport en from '../views/en.js';\nexport default {\n  components: {\n    TextTravel: TextTravel,\n    MoodleTop: MoodleTop,\n    ProfilPhoto: ProfilPhoto\n  },\n  setup: function setup() {\n    var store = useStore();\n    var idLa = computed(function () {\n      return store.state.lang.curLang;\n    });\n    var cLang = ref(idLa.value === 'fr' ? fr : en);\n    var switchLang = function switchLang() {\n      if (idLa.value === 'fr') {\n        store.commit('setLang', 'en');\n      } else if (idLa.value === 'en') {\n        store.commit('setLang', 'fr');\n      }\n    };\n    watch(idLa, function (newLang) {\n      cLang.value = newLang === 'fr' ? fr : en;\n    });\n    return {\n      switchLang: switchLang,\n      cLang: cLang,\n      pp: require('../assets/profil.png')\n    };\n  }\n};","map":{"version":3,"names":["MoodleTop","ProfilPhoto","TextTravel","useStore","computed","watch","ref","fr","en","components","setup","store","idLa","state","lang","curLang","cLang","value","switchLang","commit","newLang","pp","require"],"sources":["/home/koluyantrez/Documents/bac3/gl/Projet-developpement-logiciel-2024-main/src/main/java/com/genieLogiciel/Umons/frontend/src/elements/TopProf.vue"],"sourcesContent":["<template>\n    <MoodleTop/>\n    \n    \n    <div class=\"te\">\n    <div class=\"pic\">\n      <router-link to=\"/prof/profil\">\n        <ProfilPhoto :src=\"pp\"/>\n      </router-link>\n    </div>\n\n      <router-link to=\"/prof\">\n    <div class=\"cours\"><TextTravel :word=\"cLang.Top.course\"/></div>\n     </router-link>\n    <div class=\"loc\"><TextTravel :word=\"cLang.Top.loc\"/></div>\n    <div class=\"dospii\"><TextTravel word=\"DOSPII\"/></div>\n    </div>\n</template>\n<script>\n  import MoodleTop from './MoodleTop.vue';\n  import ProfilPhoto from './ProfilPhoto.vue';\n  import TextTravel from './TextTravel.vue';\n  import { useStore } from 'vuex';\n  import { computed, watch, ref } from 'vue';\n  import fr from '../views/fr.js';\n  import en from '../views/en.js';\n  export default {\n    components: { TextTravel, MoodleTop, ProfilPhoto },\n    setup() {\n        const store = useStore();\n        const idLa = computed(() => store.state.lang.curLang);\n        const cLang = ref(idLa.value === 'fr' ? fr : en);\n        const switchLang = () => {\n            if (idLa.value === 'fr') {\n                store.commit('setLang', 'en');\n            }\n            else if (idLa.value === 'en') {\n                store.commit('setLang', 'fr');\n            }\n        };\n\n        watch(idLa, (newLang) => {\n            cLang.value = newLang === 'fr' ? fr : en;\n        });\n\n        return {\n            switchLang,\n            cLang,\n            pp: require('../assets/profil.png'),\n        };\n    }\n  }\n</script>\n\n<style scoped>\n\n\n.pic{\n  position: absolute; \n  top: 0rem; \n  right: 0rem;\n  width: 100rem; \n  height: auto;\n  z-index: 91;\n}\n.cours{\n  position: fixed; \n  top: -2rem; \n  right: 18rem; \n  width: 5rem; \n  height: auto;\n  z-index: 91;\n  color:azure;\n}\n\n.loc{\n  position: absolute; \n  top: -2rem;\n  right: 29rem; \n  width: 6rem; \n  height: auto;\n  z-index: 91;\n}\n\n\n.dospii{\n  position: absolute; \n  top: -2rem; \n  right: 42rem; \n  width: 5rem; \n  height: auto;\n  z-index: 91;\n}\n\n.te{\n  font-family: Roboto, sans-serif;\n  color:azure;\n}\n</style> "],"mappings":"AAmBE,OAAOA,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,WAAU,MAAO,mBAAmB;AAC3C,OAAOC,UAAS,MAAO,kBAAkB;AACzC,SAASC,QAAO,QAAS,MAAM;AAC/B,SAASC,QAAQ,EAAEC,KAAK,EAAEC,GAAE,QAAS,KAAK;AAC1C,OAAOC,EAAC,MAAO,gBAAgB;AAC/B,OAAOC,EAAC,MAAO,gBAAgB;AAC/B,eAAe;EACbC,UAAU,EAAE;IAAEP,UAAU,EAAVA,UAAU;IAAEF,SAAS,EAATA,SAAS;IAAEC,WAAU,EAAVA;EAAY,CAAC;EAClDS,KAAK,WAAAA,MAAA,EAAG;IACJ,IAAMC