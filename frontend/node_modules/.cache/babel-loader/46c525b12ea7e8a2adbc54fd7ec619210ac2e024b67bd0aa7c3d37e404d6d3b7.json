{"ast":null,"code":"import ItemAdd from '../elements/ItemAdd.vue';\nimport { useStore } from 'vuex';\nimport { computed, watch, ref } from 'vue';\nimport fr from '../views/fr.js';\nimport en from '../views/en.js';\nimport axios from 'axios';\nexport default {\n  components: {\n    ItemAdd: ItemAdd\n  },\n  props: ['ToCreatePopup', 'getStudents'],\n  setup: function setup() {\n    var student = ref({\n      firstName: '',\n      lastName: '',\n      departement: '',\n      adresse: '',\n      filiere: '',\n      numero: ''\n    });\n    var createStudent = function createStudent() {\n      console.log('Student:', student.value);\n      axios.post('http://localhost:1937/AddStudent', student.value, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(function () {\n        console.log('Student created');\n        student.value = {\n          firstName: '',\n          lastName: '',\n          departement: '',\n          adresse: '',\n          filiere: '',\n          numero: ''\n        };\n      })[\"catch\"](function (error) {\n        console.error(error);\n      });\n    };\n    var store = useStore();\n    var idLa = computed(function () {\n      return store.state.lang.curLang;\n    });\n    var cLang = ref(idLa.value === 'fr' ? fr : en);\n    watch(idLa, function (newLang) {\n      cLang.value = newLang === 'fr' ? fr : en;\n    });\n    return {\n      student: student,\n      createStudent: createStudent,\n      cLang: cLang\n    };\n  }\n};","map":{"version":3,"names":["ItemAdd","useStore","computed","watch","ref","fr","en","axios","components","props","setup","student","firstName","lastName","departement","adresse","filiere","numero","createStudent","console","log","value","post","headers","then","error","store","idLa","state","lang","curLang","cLang","newLang"],"sources":["/home/koluyantrez/Documents/bac3/gl/Projet-developpement-logiciel-2024-main/src/main/java/com/genieLogiciel/Umons/frontend/src/popup/AddStudent.vue"],"sourcesContent":["<template>\n  <div class=\"popup\">\n    <div class=\"inner\">\n      <slots></slots>\n      <center>\n        <input v-model=\"student.firstName\" :placeholder=\"cLang.AddStudent.fn\" />\n        <input v-model=\"student.lastName\" :placeholder=\"cLang.AddStudent.ln\" />\n        <input v-model=\"student.departement\" :placeholder=\"cLang.AddStudent.dp\" />\n        <input v-model=\"student.filiere\" :placeholder=\"cLang.AddStudent.fl\" />\n        <input v-model=\"student.numero\" :placeholder=\"cLang.AddStudent.nu\" />\n        <input v-model=\"student.adresse\" :placeholder=\"cLang.AddStudent.ad\" />\n\n        <ItemAdd :word=\"cLang.AddCours.ok\" @click=\"createStudent\" />\n        <ItemAdd class=\"close\" :word=\"cLang.AddCours.back\" @click=\"ToCreatePopup()\" />\n      </center>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ItemAdd from '../elements/ItemAdd.vue';\nimport { useStore } from 'vuex';\nimport { computed, watch, ref } from 'vue';\nimport fr from '../views/fr.js';\nimport en from '../views/en.js';\nimport axios from 'axios';\n\nexport default {\n  components: { ItemAdd },\n  props: ['ToCreatePopup', 'getStudents'],\n\n  setup() {\n    const student = ref({\n      firstName: '',\n      lastName: '',\n      departement: '',\n      adresse: '',\n      filiere: '',\n      numero: '',\n    });\n\n    const createStudent = () => {\n      console.log('Student:', student.value);\n      axios\n          .post('http://localhost:1937/AddStudent', student.value, {\n            headers: {\n              'Content-Type': 'application/json',\n            },\n          })\n          .then(() => {\n            console.log('Student created');\n            student.value = {\n              firstName: '',\n              lastName: '',\n              departement: '',\n              adresse: '',\n              filiere: '',\n              numero: '',\n            };\n          })\n          .catch((error) => {\n            console.error(error);\n          });\n    };\n\n    const store = useStore();\n    const idLa = computed(() => store.state.lang.curLang);\n    const cLang = ref(idLa.value === 'fr' ? fr : en);\n\n    watch(idLa, (newLang) => {\n      cLang.value = newLang === 'fr' ? fr : en;\n    });\n\n    return {\n      student,\n      createStudent,\n      cLang,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.popup {\n  position: fixed;\n  top: 0px;\n  bottom: 0px;\n  left: 0px;\n  right: 0px;\n  z-index: 99;\n  background-color: rgba(0, 0, 0, 0.5);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.inner {\n  position: relative;\n  background: rgb(255, 255, 255);\n  padding: 39px;\n  border-radius: 10%;\n}\n\n.po {\n  font-size: 30px;\n  color: rgb(134, 10, 10);\n}\n</style>"],"mappings":"AAoBA,OAAOA,OAAM,MAAO,yBAAyB;AAC7C,SAASC,QAAO,QAAS,MAAM;AAC/B,SAASC,QAAQ,EAAEC,KAAK,EAAEC,GAAE,QAAS,KAAK;AAC1C,OAAOC,EAAC,MAAO,gBAAgB;AAC/B,OAAOC,EAAC,MAAO,gBAAgB;AAC/B,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE;IAAER,OAAM,EAANA;EAAQ,CAAC;EACvBS,KAAK,EAAE,CAAC,eAAe,EAAE,aAAa,CAAC;EAEvCC,KAAK,WAAAA,MAAA,EAAG;IACN,IAAMC,OAAM,GAAIP,GAAG,CAAC;MAClBQ,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,EAAE;MACfC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,EAAE;MACXC,MAAM,EAAE;IACV,CAAC,CAAC;IAEF,IAAMC,aAAY,GAAI,SAAhBA,aAAYA,CAAA,EAAU;MAC1BC,OAAO,CAACC,GAAG,CAAC,UAAU,EAAET,OAAO,CAACU,KAAK,CAAC;MACtCd,KAAI,CACCe,IAAI,CAAC,kCAAkC,EAAEX,OAAO,CAACU,KAAK,EAAE;QACvDE,OAAO,EAAE;UACP,cAAc,EAAE;QAClB;MACF,CAAC,EACAC,IAAI,CAAC,YAAM;QACVL,OAAO,CAACC,GAAG,CAAC,iBAAiB,CAAC;QAC9BT,OAAO,CAACU,KAAI,GAAI;UACdT,SAAS,EAAE,EAAE;UACbC,QAAQ,EAAE,EAAE;UACZC,WAAW,EAAE,EAAE;UACfC,OAAO,EAAE,EAAE;UACXC,OAAO,EAAE,EAAE;UACXC,MAAM,EAAE;QACV,CAAC;MACH,CAAC,UACK,CAAC,UAACQ,KAAK,EAAK;QAChBN,OAAO,CAACM,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACR,CAAC;IAED,IAAMC,KAAI,GAAIzB,QAAQ,CAAC,CAAC;IACxB,IAAM0B,IAAG,GAAIzB,QAAQ,CAAC;MAAA,OAAMwB,KAAK,CAACE,KAAK,CAACC,IAAI,CAACC,OAAO;IAAA,EAAC;IACrD,IAAMC,KAAI,GAAI3B,GAAG,CAACuB,IAAI,CAACN,KAAI,KA