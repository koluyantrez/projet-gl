{"ast":null,"code":"import ItemAdd from '../elements/ItemAdd.vue';\nimport { useStore } from 'vuex';\nimport { computed, ref, watch } from 'vue';\nimport axios from 'axios';\nimport fr from '../views/fr.js';\nimport en from '../views/en.js';\nimport Cookies from 'js-cookie';\nexport default {\n  components: {\n    ItemAdd: ItemAdd\n  },\n  props: ['ToPicPopup'],\n  setup: function setup() {\n    var store = useStore();\n    var idLa = computed(function () {\n      return store.state.lang.curLang;\n    });\n    var cLang = ref(idLa.value === 'fr' ? fr : en);\n    watch(idLa, function (newLang) {\n      cLang.value = newLang === 'fr' ? fr : en;\n    });\n    var uploadedFile = ref(null);\n    var file = ref(null);\n    var handleFileUpload = function handleFileUpload(event) {\n      file.value = event.target.files[0];\n      if (file.value) {\n        var formData = new FormData();\n        formData.append('file', file.value);\n        var matricule = Cookies.get('matriculeProfesseur') || Cookies.get('matriculeInscription') || ''; // Ajoutez ici les autres matricules si nécessaire\n\n        if (matricule) {\n          axios.post(\"http://localhost:1937/users/\".concat(matricule, \"/uploadImage\"), formData).then(function (response) {\n            console.log('Image uploaded successfully');\n            // Rafraîchissement de la page pour afficher la nouvelle image\n            window.location.reload();\n          })[\"catch\"](function (error) {\n            console.error('Error uploading image:', error);\n          });\n        } else {\n          console.error('No matricule found in cookies');\n        }\n      }\n    };\n    return {\n      cLang: cLang,\n      uploadedFile: uploadedFile,\n      handleFileUpload: handleFileUpload\n    };\n  }\n};","map":{"version":3,"names":["ItemAdd","useStore","computed","ref","watch","axios","fr","en","Cookies","components","props","setup","store","idLa","state","lang","curLang","cLang","value","newLang","uploadedFile","file","