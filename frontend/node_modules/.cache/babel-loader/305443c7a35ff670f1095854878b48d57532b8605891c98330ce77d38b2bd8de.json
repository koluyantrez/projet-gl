{"ast":null,"code":"import MoodleTop from './MoodleTop.vue';\nimport { useStore } from 'vuex';\nimport { computed, watch, ref, onMounted } from 'vue';\nimport fr from '../views/fr.js';\nimport en from '../views/en.js';\nimport axios from 'axios';\nimport Cookies from 'js-cookie';\nexport default {\n  components: {\n    MoodleTop: MoodleTop\n  },\n  setup: function setup() {\n    var matricule = ref(Cookies.get('matriculeStudent'));\n    var pic = ref(null);\n    var getPic = function getPic() {\n      axios.get('http://localhost:1937/students/findById', {\n        params: {\n          matricule: matricule.value\n        }\n      }).then(function (response) {\n        var info = response.data;\n        pic.value = \"data:image/jpeg;base64,\".concat(info.image);\n      })[\"catch\"](function (error) {\n        console.error(error);\n      });\n    };\n    onMounted(function () {\n      getPic();\n    });\n    var store = useStore();\n    var idLa = computed(function () {\n      return store.state.lang.curLang;\n    });\n    var cLang = ref(idLa.value === 'fr' ? fr : en);\n    watch(idLa, function (newLang) {\n      cLang.value = newLang === 'fr' ? fr : en;\n    });\n    return {\n      cLang: cLang,\n      pic: pic\n    };\n  }\n};","map":{"version":3,"names":["MoodleTop","useStore","computed","watch","ref","onMounted","fr","en","axios","Cookies","components","setup","matricule","get","pic","getPic","params","value","then","response","info","data","concat","image","error","console","store","idLa","state","lang","curLang","cLang","newLang"],"sources":["C:\\Users\\trist\\Desktop\\projet-gl-main\\frontend\\src\\elements\\TopStudent.vue"],"sourcesContent":["<template>\n    <MoodleTop/>\n    <router-link to=\"/student/profil\">\n        <img class=\"pic\" :src=\"pic\"/>\n    </router-link>\n\n    <div class=\"te\">\n      <router-link to=\"/student\">\n        <div class=\"cours\">{{cLang.Top.course}}</div>\n      </router-link>\n      <div class=\"loc\">{{cLang.Top.loc}}</div>\n      <div class=\"bu\">Bulletin</div>\n      <router-link to=\"/dospii\">\n        <div class=\"dospii\">DOSPII</div>\n      </router-link>\n    </div>\n  \n</template>\n<script>\nimport MoodleTop from './MoodleTop.vue';\nimport { useStore } from 'vuex';\nimport { computed, watch, ref, onMounted } from 'vue';\nimport fr from '../views/fr.js';\nimport en from '../views/en.js';\nimport axios from 'axios';\nimport Cookies from 'js-cookie';\n\nexport default {\n  components: { MoodleTop },\n\n  setup() {\n    const matricule = ref(Cookies.get('matriculeStudent'));\n    const pic = ref(null);\n\n    const getPic = () => {\n      axios.get('http://localhost:1937/students/findById', {\n        params: { matricule: matricule.value }\n      })\n      .then(response => {\n        const info = response.data;\n        pic.value = `data:image/jpeg;base64,${info.image}`;\n      })\n      .catch(error => {\n        console.error(error);\n      });\n    };\n\n    onMounted(() => {\n      getPic();\n    });\n\n    const store = useStore();\n    const idLa = computed(() => store.state.lang.curLang);\n    const cLang = ref(idLa.value === 'fr' ? fr : en);\n\n    watch(idLa, (newLang) => {\n      cLang.value = newLang === 'fr' ? fr : en;\n    });\n\n    return {\n      cLang,\n      pic\n    };\n  }\n};\n</script>\n\n<style scoped>\n\n.la{\n  position: absolute;\n  top: 1rem;\n  left: 13rem;\n  width: 4rem; \n  height: auto;\n  z-index: 91;\n}\n\n.pic{\n  position: absolute; \n  top: 0.5rem;\n  right: 1rem;\n  width: 5rem;\n  height: 5rem;\n  z-index: 92;\n}\n\n.te{\n  position: relative;\n  top: 0rem;\n  font-family: Roboto, sans-serif;\n  font-size: 60px;\n  color: azure;\n  text-align: center;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  z-index: 92;\n}\n\n.cours,\n.loc,\n.bu,\n.dospii {\n  position: absolute;\n  color:azure;\n}\n\n.cours{\n  top: 0;\n  right: 20rem;\n  width: 5rem; \n  height: auto;\n}\n\n.loc{\n  right: 32rem;\n  width: 6rem; \n  height: auto;\n}\n\n.bu{\n  right: 48rem;\n  width: 5rem; \n  height: auto;\n}\n\n.dospii{\n  right: 62rem;\n  width: 5rem; \n  height: auto;\n}\n\n\n\n\n</style> \n"],"mappings":"AAmBA,OAAOA,SAAQ,MAAO,iBAAiB;AACvC,SAASC,QAAO,QAAS,MAAM;AAC/B,SAASC,QAAQ,EAAEC,KAAK,EAAEC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACrD,OAAOC,EAAC,MAAO,gBAAgB;AAC/B,OAAOC,EAAC,MAAO,gBAAgB;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,WAAW;AAE/B,eAAe;EACbC,UAAU,EAAE;IAAEV,SAAQ,EAARA;EAAU,CAAC;EAEzBW,KAAK,WAAAA,MAAA,EAAG;IACN,IAAMC,SAAQ,GAAIR,GAAG,CAACK,OAAO,CAACI,GAAG,CAAC,kBAAkB,CAAC,CAAC;IACtD,IAAMC,GAAE,GAAIV,GAAG,CAAC,IAAI,CAAC;IAErB,IAAMW,MAAK,GAAI,SAATA,MAAKA,CAAA,EAAU;MACnBP,KAAK,CAACK,GAAG,CAAC,yCAAyC,EAAE;QACnDG,MAAM,EAAE;UAAEJ,SAAS,EAAEA,SAAS,CAACK;QAAM;MACvC,CAAC,EACAC,IAAI,CAAC,UAAAC,QAAO,EAAK;QAChB,IAAMC,IAAG,GAAID,QAAQ,CAACE,IAAI;QAC1BP,GAAG,CAACG,KAAI,6BAAAK,MAAA,CAA8BF,IAAI,CAACG,KAAK,CAAE;MACpD,CAAC,UACK,CAAC,UAAAC,KAAI,EAAK;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACJ,CAAC;IAEDnB,SAAS,CAAC,YAAM;MACdU,MAAM,CAAC,CAAC;IACV,CAAC,CAAC;IAEF,IAAMW,KAAI,GAAIzB,QAAQ,CAAC,CAAC;IACxB,IAAM0B,IAAG,GAAIzB,QAAQ,CAAC;MAAA,OAAMwB,KAAK,CAACE,KAAK,CAACC,IAAI,CAACC,OAAO;IAAA,EAAC;IACrD,IAAMC,KAAI,GAAI3B,GAAG,CAACuB,IAAI,CAACV,KAAI,KAAM,IAAG,GAAIX,EAAC,GAAIC,EAAE,CAAC;IAEhDJ,KAAK,CAACwB,IAAI,EAAE,UAACK,OAAO,EAAK;MACvBD,KAAK,CAACd,KAAI,GAAIe,OAAM,KAAM,IAAG,GAAI1B,EAAC,GAAIC,EAAE;IAC1C,CAAC,CAAC;IAEF,OAAO;MACLwB,KAAK,EAALA,KAAK;MACLjB,GAAE,EAAFA;IACF,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}