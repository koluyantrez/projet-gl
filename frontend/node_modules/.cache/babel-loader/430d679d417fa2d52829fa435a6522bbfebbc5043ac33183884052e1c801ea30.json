herName;\n        console.log(info.teacherName);\n        axios.get(\"http://localhost:1937/api/professeurs/findByName?teacherName=\".concat(_this.prof)).then(function (response) {\n          var sensei = response.data;\n          _this.emailProf = sensei.email;\n        })[\"catch\"](function (error) {\n          console.error(error);\n        });\n      })[\"catch\"](function (error) {\n        console.error(error);\n      });\n    },\n    getStudentName: function getStudentName() {\n      var _this2 = this;\n      var role = Cookies.get('role');\n      var matricule = '';\n      if (role === 'professeur') {\n        matricule = Cookies.get('matriculeProfesseur');\n        axios.get(\"http://localhost:1937/teachers/findById?matricule=\".concat(matricule)).then(function (response) {\n          var teacher = response.data;\n          _this2.studentName = teacher.name; // Mettre à jour le nom du professeur\n          console.log(teacher.name);\n        })[\"catch\"](function (error) {\n          console.error(error);\n        });\n      } else if (role === 'student') {\n        matricule = Cookies.get('matriculeStudent');\n        axios.get(\"http://localhost:1937/students/findById?matricule=\".concat(matricule)).then(function (response) {\n          var student = response.data;\n          _this2.studentName = student.name; // Mettre à jour le nom de l'étudiant\n          console.log(response.data);\n        })[\"catch\"](function (error) {\n          console.error(error);\n        });\n      }\n    },\n    getCookie: function getCookie(name) {\n      var value = \"; \".concat(document.cookie);\n      var parts = value.split(\"; \".concat(name, \"=\"));\n      if (parts.length === 2) {\n        return parts.pop().split(';').shift();\n      }\n      return null;\n    }\n  }\n};","map":{"version":3,"names":["TopStudent","TopProf","TopGuest","Cours","axios","Cookies","useStore","computed","watch","ref","fr","en","components","data","store","idLa","state","lang","curLang