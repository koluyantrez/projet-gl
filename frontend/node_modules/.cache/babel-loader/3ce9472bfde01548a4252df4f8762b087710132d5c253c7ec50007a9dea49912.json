{"ast":null,"code":"import MoodleTop from './MoodleTop.vue';\nimport { useStore } from 'vuex';\nimport { computed, watch, ref, onMounted } from 'vue';\nimport fr from '../views/fr.js';\nimport en from '../views/en.js';\nimport axios from 'axios';\nimport Cookies from 'js-cookie';\nexport default {\n  components: {\n    MoodleTop: MoodleTop\n  },\n  setup: function setup() {\n    var store = useStore();\n    var idLa = computed(function () {\n      return store.state.lang.curLang;\n    });\n    var cLang = ref(idLa.value === 'fr' ? fr : en);\n    console.log(cLang.value);\n    watch(idLa, function (newLang) {\n      cLang.value = newLang === 'fr' ? fr : en;\n    });\n    var matricule = ref(Cookies.get('matriculeInscription'));\n    var pic = ref(null);\n    var getPic = function getPic() {\n      axios.get('http://localhost:1937/inscription/membreServiceInscription', {\n        params: {\n          matricule: matricule.value\n        }\n      }).then(function (response) {\n        var info = response.data;\n        pic.value = \"data:image/jpeg;base64,\".concat(info.image);\n      })[\"catch\"](function (error) {\n        console.error(error);\n      });\n    };\n    onMounted(function () {\n      getPic();\n    });\n    return {\n      cLang: cLang,\n      pic: pic\n    };\n  }\n};","map":{"version":3,"names":["MoodleTop","useStore","computed","watch","ref","onMounted","fr","en","axios","Cookies","components","setup","store","idLa","state","lang","curLang","cLang","value","console","log","newLang","matricule","get","pic","getPic","params","then","response","info","data","concat","image","error"],"sources":["C:\\Users\\trist\\Desktop\\projet-gl-main\\frontend\\src\\elements\\TopSec.vue"],"sourcesContent":["<template>\n  <MoodleTop/>\n  <div class=\"te\">\n\n    <div class=\"pic\">\n      <router-link to=\"/sec/profil\">\n        <img class=\"pic\" :src=\"pic\"/>\n      </router-link>\n    </div>\n    <router-link to=\"/ins\">\n      <div class=\"la\">{{ cLang.Top.signin }}</div>\n    </router-link>\n    <router-link to=\"/sec\">\n      <div class=\"loc\">{{ cLang.Top.course }}</div>\n    </router-link>\n    <div class=\"cours\">{{ cLang.Top.loc }}</div>\n  </div>\n</template>\n\n<script>\n  import MoodleTop from './MoodleTop.vue';\n  import { useStore } from 'vuex';\n  import { computed, watch, ref, onMounted } from 'vue';\n  import fr from '../views/fr.js';\n  import en from '../views/en.js';\n  import axios from 'axios';\n  import Cookies from 'js-cookie';\n\n  export default {\n    components: { MoodleTop },\n    setup() {\n        const store = useStore();\n        const idLa = computed(() => store.state.lang.curLang);\n        const cLang = ref(idLa.value === 'fr' ? fr : en);\n        console.log(cLang.value);\n        watch(idLa, (newLang) => {\n            cLang.value = newLang === 'fr' ? fr : en;\n        });\n\n\n        const matricule = ref(Cookies.get('matriculeInscription'));\n        const pic = ref(null);\n\n        const getPic = () => {\n          axios.get('http://localhost:1937/inscription/membreServiceInscription', {\n              params: { matricule: matricule.value }\n          })\n              .then(response => {\n                const info = response.data;\n                pic.value = `data:image/jpeg;base64,${info.image}`;\n              })\n              .catch(error => {\n                console.error(error);\n              });\n            };\n\n        onMounted(() => {\n              getPic();\n        });\n\n        return {\n            cLang,\n            pic\n        };\n    }\n  }\n</script>\n\n<style scoped>\n.pic {\n  position: absolute;\n  top: 0.1rem;\n  right: 0rem;\n  width: 5rem;\n  height: 5rem;\n  z-index: 92;\n}\n\n.te .la {\n  position: fixed;\n  top: 0.5rem;\n  right: 55rem;\n  width: 5rem;\n  height: auto;\n  z-index: 91;\n  color: azure;\n}\n\n.te .cours {\n  position: fixed;\n  top: 0.5rem;\n  right: 19rem;\n  width: 5rem;\n  height: auto;\n  z-index: 91;\n  color: azure;\n}\